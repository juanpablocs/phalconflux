include ../config/config.jade
//- layout.jade
doctype html
html
	head
		meta(charset="UTF-8")
		title
			!= config.page.title
		meta(revisit-after="1 Days")
		meta(robots="index, follow")
		meta(name="description", content!="#{config.page.description}")
		meta(name="keywords", content="Keywords")
		link(href="/static/css/vendor.min.css")
		link(href="/static/css/main.min.css")
	body
		block content

		script(src='/static/js/vendor.min.js')
		script(src='/static/js/main.min.js')